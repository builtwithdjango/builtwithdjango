{
  "name": "builtwithdjango",
  "version": "1.0.0",
  "description": "Collection of app built with Django",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rasulkireev/builtwithdjango.git"
  },
  "scripts": {
    "develop": "npx webpack && poetry run python manage.py migrate && poetry run python manage.py collectstatic && python manage.py runserver",
